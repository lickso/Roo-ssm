<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  

<mapper namespace="com.roo.ssm.dao.RoleDao">

	<!-- 根据用户id查询用户信息 -->
    <select id="queryObjectByID" parameterType="int" resultType="java.lang.String">   
